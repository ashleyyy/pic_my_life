<div class ="white-space-on-top">
 <h3>More photos from: <a href="/users/<%= @user.id %>"><%= user.username %></a></h3>
</div>

<div class="photowall"> 
  <div id="masonry-grid">
    <div class="grid-sizer">

    <!-- START MORE PHOTOS LOOP -->
    <% @photoroll.each do |photo| %>
        <a href = "/photos/<%= photo.id %>">
          <div class="grid-item xs">
            <img src="<%= photo.file %>" />
         </div>
        </a>
    <% end %>
    <!-- END MORE PHOTOS LOOP-->

    </div> 
  </div>
</div>
