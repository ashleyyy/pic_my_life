<div class="photowall"> 
  <div id="masonry-grid">
    <div class="grid-sizer">

<!-- START PHOTO LOOP -->
    <% @photos.each do |photo| %>
        <div class="thumb grid-item">
          <img src="<%= photo.file %>" />
          <div>
            <h3 class="meme-font text-center">
              <a href ="/photos/<%= photo.id %>/"><%= photo.caption %></a>
            </h3>
            <div>
              <%= erb :'vote/_vote', locals: { photo: photo } %>
            </div>
          </div>
        </div>
    <% end %>
<!-- END PHOTO loop -->

    </div>
  </div>
</div>
